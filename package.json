{
  "name": "pihanga",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "apollo-cache-inmemory": "^1.3.6",
    "apollo-client": "^2.4.3",
    "apollo-link": "^1.2.3",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "graphql-type-json": "^0.2.1",
    "history": "^4.5.1",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.8",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-redux": "^6.0.1",
    "react-scripts": "2.0.4",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.0.1",
    "postcss-cli": "^6.0.0",
    "react-test-renderer": "^16.5.2",
    "tailwindcss": "^0.6.6"
  },
  "scripts": {
    "build:css": "postcss src/apollo-client-example-app/ui/app-router.component.css -o src/shared/auto-generated.app-router.component.css",
    "watch:css": "postcss src/apollo-client-example-app/ui/app-router.component.css -o src/apollo-client-example-app/ui/auto-generated.app-router.component.css",
    "lint": "npm run build:css && eslint --ext .jsx,.js src/",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 30,
        "branches": 30,
        "functions": 30,
        "lines": 30
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ]
  }
}
