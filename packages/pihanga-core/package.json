{
  "name": "core",
  "version": "0.5.0",
  "description": "Core module of the Pihanga framework",
  "homepage": "https://github.com/n1analytics/pihanga#readme",
  "main": "dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n1analytics/pihanga.git"
  },
  "bugs": "https://github.com/n1analytics/pihanga/issues",
  "keywords": [
    "UI",
    "framework"
  ],
  "contributors": [
    "Max Ott <max@winlab.rutgers.edu> (http://linkedin.com/in/max-ott)",
    "Pihanga Team"
  ],
  "license": "Undefined",
  "unpkg": "dist/pihanga-core.js",
  "module": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib && touch lib/DO_NOT_EDIT",
    "build:es": "babel src --out-dir dist/es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/pihanga-core.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/pihanga-core.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf dist coverage",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" index.d.ts \"docs/**/*.md\"",
    "lint": "eslint src test/utils test/components",
    "prepare": "npm run clean && npm run build",
    "pretest": "npm run lint",
    "test": "node ./test/run-tests.js",
    "coverage": "codecov"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^6.0.1",
    "lodash.every": "^4.6.0",
    "lodash.isplainobject": "4.0.6",
    "lodash.isarray": "4.0.0",
    "lodash.isfunction": "3.0.9",
    "lodash.isobject": "3.0.2",
    "lodash.isstring": "4.0.1",
    "lodash.isequal": "4.5.0",
    "history": "^4.9.0",
    "stackinfo": "^1.1.5",
    "redux-thunk": "^2.3.0",
    "path-to-regexp": "^3.0.0",
    "numeral": "^2.0.6",
    "whatwg-fetch": "~3.0.0",
    "moment": "^2.24.0",
    "humanize-duration": "^3.18.0",
    "prop-types": "^15.7.2",
    "@babel/runtime": "^7.3.1"
  },
  "peerDependencies": {
    "react": "^16.4.0-0",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "codecov": "^3.2.0",
    "create-react-class": "^15.6.3",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "es3ify": "^0.2.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "npm-run": "^5.0.1",
    "prettier": "^1.16.4",
    "react-testing-library": "^5.9.0",
    "redux": "^4.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "semver": "^5.6.0"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
